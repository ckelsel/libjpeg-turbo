#!/bin/sh

set -e

if [ remove = "$1" -o abort-install = "$1" -o disappear = "$1" ];
then
	dpkg-divert --package libturbojpeg62 --rename \
		--remove /usr/lib/libjpeg.so.62.0.0 
	dpkg-divert --package libturbojpeg62 --rename \
		--remove /usr/lib/libjpeg.so.62 
	rmdir /usr/lib/libjpeg-divert
fi

